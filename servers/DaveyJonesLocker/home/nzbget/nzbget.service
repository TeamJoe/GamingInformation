#/etc/systemd/system
[Unit]
Description=NZBGet Usenet Client
After=docker.service

[Service]
Type=oneshot
RemainAfterExit=yes
User=nzbget
Group=docker
ExecStart=/home/nzbget/nzbget.sh start
ExecStop=/home/nzbget/nzbget.sh stop
StandardOutput=syslog
StandardError=syslog

[Install]
WantedBy=multi-user.target
